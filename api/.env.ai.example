# AI-Powered Search Configuration
# Copy these variables to your .env file and configure as needed

# AI Provider Selection
# Options: gemini, openai, claude, fallback
# fallback = keyword-based extraction (no API key needed)
AI_PROVIDER=gemini
AI_ENABLED=true
AI_AUTO_ANALYZE=false

# Google Gemini Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.5-pro

# OpenAI Configuration (if using OpenAI)
# OPENAI_API_KEY=your_openai_api_key_here
# OPENAI_MODEL=gpt-4-turbo-preview
# OPENAI_ORGANIZATION=your_org_id

# Anthropic Claude Configuration (if using Claude)
# CLAUDE_API_KEY=your_claude_api_key_here
# CLAUDE_MODEL=claude-3-sonnet-20240229

# AI Processing Configuration
AI_TIMEOUT=60
AI_MODEL=gemini-2.5-pro

# Queue Configuration (for background AI processing)
QUEUE_CONNECTION=redis
REDIS_HOST=redis
REDIS_PORT=6379
